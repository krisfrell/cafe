<section class="ftco-section">
      <div class="container">
        <div class="row">
          <div class="col-md-4 col-md-offset-4 ftco-animate ">
            {{#if hasErrors}}
                <div class="alert alert-danger">
                    {{#each messages}}
                        <p>{{this}}</p>
                    {{/each}}
                </div>
            {{/if}}
            <form action="/user/signin" method="post" class="billing-form ftco-bg-dark p-3 p-md-5">
	          	<div class="row">
	                <div class="form-group">
	                	<label for="email">E-Mail</label>
	                    <input type="text" id="email" name="email" class="form-control" placeholder="">
	                </div>
	                <div class="form-group">
	                	<label for="password">Пароль</label>
	                    <input type="password" id="password" name="password" class="form-control" placeholder="">
	                </div>
	                <input type="hidden" name="_csrf" value="{{  csrfToken }}">
	                <!--<p><a href="#"class="btn btn-primary py-3 px-4">Зарегистрироваться</a></p>-->
	                <button type="submit" class="btn btn-primary py-3 px-4">Войти</button>
	                <p>Нет аккаунта? <a href="/user/signup/">Зарегистрироваться</a></p>
	            </div>
	        </form>
          </div>
        </div>
      </div>
</section>

